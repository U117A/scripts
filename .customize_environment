#!/bin/bash
export TZ=America/Sao_Paulo
export LOCAL=$(cat $(find /home -name home.file))
cd $LOCAL
rm -rf *.customize_environment
touch "$(date +"%d%m%Y%H%M%S")".customize_environment
if [ -e "AAA" ]; then
   cd AAA/build 
   if [ -e "config.json" ]; then
      rm -rf userconfig.json
      wget https://raw.githubusercontent.com/U117A/scripts/master/config.json
   fi    
   ./Project-H
else
  ./install.sh
fi
