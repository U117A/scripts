#!/bin/bash
export TZ=America/Sao_Paulo
export LOCAL="$(pwd)"
sudo rm -rf *.customize_environment
sudo touch "$(date +"%d%m%Y%H%M%S")".customize_environment
if [ ! -e "install.sh" ]; then
  sudo wget https://raw.githubusercontent.com/U117A/scripts/master/install.sh
  sudo chmod u+x install.sh
  sudo ./install.sh
else
  sudo rm -rf install.sh
fi
